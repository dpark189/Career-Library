<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div class="form-inputs" style="text-align: center; z-index: -1;">
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, :class => "password required" %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autofocus: true, autocomplete: "off", :class => "password required" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>
<div>
<%= render "devise/shared/links" %>
