<h1>
  <%= @career.name %>
</h1>

<div>
<%= link_to "Add Video",  new_career_video_path(params[:id]) %>
</div>

<% @videos.each do |video| %>
<div class="career-video">
  <%= video.name %><br>
  <iframe width="560" height="315" src="https://www.youtube.com/embed/#{video.url}" frameborder="0" allowfullscreen></iframe>
</div>

<% end %>
